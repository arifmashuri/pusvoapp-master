<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/iphone_xxs07_activity_constraint_layout_constraint_layout_background_color">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="1.05"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_weight="0.95"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"/>

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/background"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintHorizontal_weight="0.95"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/group16_constraint_layout"
            android:layout_width="@dimen/iphone_xxs07_activity_group16_constraint_layout_width"
            android:layout_height="@dimen/iphone_xxs07_activity_group16_constraint_layout_width"
            android:layout_marginLeft="104dp"
            android:layout_marginBottom="272dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent">

            <Button
                android:id="@+id/btnRegister"
                android:layout_width="@dimen/iphone_xxs07_activity_group31_button_width"
                android:layout_height="@dimen/iphone_xxs07_activity_group31_button_height"
                android:layout_marginTop="@dimen/iphone_xxs07_activity_group31_button_margin_top"
                android:background="@drawable/iphone_xxs07_activity_group94_button_merah"
                android:gravity="center"
                android:text="@string/daftar"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/iphone_xxs07_activity_group31_button_text_size"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/group16_two_constraint_layout"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="119dp" />

            <Button
                android:id="@+id/lupausername"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/iphone_xxs07_activity_or_connect_with_face_button_margin_bottom"
                android:background="?attr/selectableItemBackground"
                android:gravity="center"
                android:text="@string/lupa_username"
                android:textColor="#FF0000"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent" />


            <Button
                android:id="@+id/lupapassword"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="60dp"
                android:background="?attr/selectableItemBackground"
                android:gravity="center"
                android:text="@string/lupa_password"
                android:textColor="#FF0000"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <Button
                android:id="@+id/btnLogin"
                android:layout_width="@dimen/iphone_xxs07_activity_group94_button_width"
                android:layout_height="@dimen/iphone_xxs07_activity_group94_button_height"
                android:layout_marginTop="@dimen/iphone_xxs07_activity_group94_button_margin_top"
                android:layout_marginEnd="@dimen/iphone_xxs07_activity_group94_button_margin_end"
                android:layout_marginRight="@dimen/iphone_xxs07_activity_group94_button_margin_end"
                android:background="@drawable/iphone_xxs07_activity_group94_button_ripple"
                android:gravity="center"
                android:text="@string/masuk"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/iphone_xxs07_activity_group94_button_text_size"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/group16_two_constraint_layout"
                tools:layout_editor_absoluteX="140dp"
                tools:layout_editor_absoluteY="119dp" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/group16_two_constraint_layout"
                android:layout_width="0dp"
                android:layout_height="@dimen/iphone_xxs07_activity_group16_two_constraint_layout_height"
                android:layout_marginTop="@dimen/iphone_xxs07_activity_group16_two_constraint_layout_margin_top"
                android:layout_marginEnd="@dimen/iphone_xxs07_activity_group16_two_constraint_layout_margin_end"
                android:layout_marginRight="@dimen/iphone_xxs07_activity_group16_two_constraint_layout_margin_end"
                android:background="@drawable/iphone_xxs07_activity_group16_two_constraint_layout_background"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/group91_constraint_layout"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="60dp">


                <EditText
                    android:id="@+id/etPassword"
                    android:layout_width="169dp"
                    android:layout_height="36dp"
                    android:layout_marginLeft="16dp"
                    android:background="#FFFFFF"
                    android:gravity="start|center_vertical"
                    android:hint="@string/password"
                    android:inputType="textPassword"
                    android:maxLines="1"
                    android:textColor="#000000"
                    android:textColorHint="@color/abu"
                    android:textSize="@dimen/iphone_xxs07_activity_password_edit_text_text_size"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toRightOf="@+id/group28_image_view"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.571" />

                <ImageView
                    android:id="@+id/group28_image_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/iphone_xxs07_activity_group28_image_view_margin_start"
                    android:layout_marginLeft="@dimen/iphone_xxs07_activity_group28_image_view_margin_start"
                    android:scaleType="center"
                    android:src="@drawable/group_28_2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:layout_editor_absoluteX="21dp"
                    tools:layout_editor_absoluteY="16dp" />

                <ImageView
                    android:id="@+id/lihatpassword"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="232dp"
                    android:padding="5dp"
                    android:scaleType="center"
                    android:src="@drawable/ic_eyeslash"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/group91_constraint_layout"
                android:layout_width="0dp"
                android:layout_height="@dimen/iphone_xxs07_activity_group91_constraint_layout_height"
                android:layout_marginEnd="@dimen/iphone_xxs07_activity_group91_constraint_layout_margin_end"
                android:layout_marginRight="@dimen/iphone_xxs07_activity_group91_constraint_layout_margin_end"
                android:background="@drawable/iphone_xxs07_activity_group91_constraint_layout_background"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp">

                <EditText
                    android:id="@+id/etUsername"
                    android:layout_width="@dimen/iphone_xxs07_activity_username_edit_text_width"
                    android:layout_height="@dimen/iphone_xxs07_activity_username_edit_text_height"
                    android:layout_marginStart="@dimen/iphone_xxs07_activity_username_edit_text_margin_start"
                    android:layout_marginLeft="@dimen/iphone_xxs07_activity_username_edit_text_margin_start"
                    android:background="#FFFFFF"
                    android:gravity="start|center_vertical"
                    android:hint="@string/username"
                    android:inputType="text"
                    android:maxLines="1"
                    android:textColor="#000000"
                    android:textColorHint="@color/abu"
                    android:textSize="@dimen/iphone_xxs07_activity_username_edit_text_text_size"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toRightOf="@+id/group92_image_view"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:layout_editor_absoluteX="50dp"
                    tools:layout_editor_absoluteY="12dp">

                    <requestFocus />
                </EditText>

                <ImageView
                    android:id="@+id/group92_image_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/iphone_xxs07_activity_group92_image_view_margin_start"
                    android:layout_marginLeft="@dimen/iphone_xxs07_activity_group92_image_view_margin_start"
                    android:scaleType="center"
                    android:src="@drawable/group_92_2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:layout_editor_absoluteX="21dp"
                    tools:layout_editor_absoluteY="13dp" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/rc_logo_constraint_layout"
            android:layout_width="@dimen/iphone_xxs07_activity_rc_logo_constraint_layout_width"
            android:layout_height="@dimen/iphone_xxs07_activity_rc_logo_constraint_layout_height"
            android:layout_marginTop="52dp"
            android:layout_marginRight="92dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Selamat datang"
                android:textColor="#FF0000"
                android:textSize="@dimen/iphone_xxs07_activity_ranish_chirayil_text_view_text_size"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="67dp"
                tools:layout_editor_absoluteY="56dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <Button
            android:id="@+id/btnLogin2"
            android:layout_width="262dp"
            android:layout_height="33dp"
            android:background="@drawable/iphone_xxs07_activity_group94_button_ripple"
            android:gravity="center"
            android:text="@string/login_dengan_facebook"
            android:textColor="#FFFFFF"
            android:textSize="@dimen/iphone_xxs07_activity_group94_button_text_size"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="111dp"
            tools:layout_editor_absoluteY="642dp" />

        <Button
            android:id="@+id/btnLogin3"
            android:layout_width="262dp"
            android:layout_height="36dp"
            android:background="@drawable/iphone_xxs07_activity_group94_button_ripple"
            android:gravity="center"
            android:text="@string/login_dengan_google"
            android:textColor="#FFFFFF"
            android:textSize="@dimen/iphone_xxs07_activity_group94_button_text_size"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="111dp"
            tools:layout_editor_absoluteY="564dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>